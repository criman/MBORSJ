===================================================================================================*/
#define 	SETB(r,n)				(r |= (1<<n))
#define 	CLRB(r,n)				(r &= ~(1<<n))
#define 	IFCB(r,n)				(r & (1<<n))
#define 	SWPB(r,n)				(r ^= (1<<n))

#define		HIBYTE(v1)				((Byte)((v1)>>8))
#define		LOBYTE(v1)				((Byte)((v1)&0xff))

